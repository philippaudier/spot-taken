{
  "name": "spot-taken",
  "version": "0.0.0",
  "scripts": {
    "front": "ng serve --open",
    "back": "npm run back-debug",
    "build-prod": "node ./node_modules/@angular/cli/bin/ng build --prod && npm run back-compile",
    "start-prod": "node backendDist/main.js",
    "back-compile": "rimraf backendDist && tsc -p backend/tsconfig.json",
    "back-compile-watch": "rimraf backendDist && npm run back-compile-watch-only",
    "back-compile-watch-only": "tsc -p tsconfig.json -w",
    "back-debug-only": "./node_modules/.bin/nodemon --inspect backendDist/main.js",
    "back-debug-brk-only": "./node_modules/.bin/nodemon --inspect-brk backendDist/main.js",
    "back-prod": "npm run back-compile && npm run start-prod",
    "back-debug": "npm run back-compile && concurrently -k -n \"tsc,back\" -c \"magenta,green\" \"npm:back-compile-watch-only\" \"npm:back-debug-only\"",
    "back-debug-brk": "npm run back-compile && concurrently -k -n \"back-tsc,back\" -c \"magenta,green\" \"npm:back-compile-watch-only\" \"npm:back-debug-brk-only\"",
    "start": "npm run start-prod",
    "postinstall": "npm run build-prod"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.7",
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "^8.0.2",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "~8.0.0",
    "@angular/material": "^8.0.2",
    "@angular/material-moment-adapter": "^8.1.1",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "angular-oauth2-oidc": "^8.0.1",
    "body-parser": "^1.18.3",
    "compression": "1.7.1",
    "express": "^4.16.3",
    "googleapis": "^39.2.0",
    "materialize-css": "^1.0.0",
    "moment": "^2.24.0",
    "pg": "^7.11.0",
    "rxjs": "~6.4.0",
    "source-map-support": "^0.5.12",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.1",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "codelyzer": "^5.0.0",
    "concurrently": "^4.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "nodemon": "^1.18.11",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  },
  "nodemonConfig": {
    "watch": [
      "backendDist"
    ],
    "delay": "2000"
  }
}
